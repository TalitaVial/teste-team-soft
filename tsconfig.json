{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es2015", "es2016", "es2017"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "jsx": "react"
    },
    "include": ["src"]
  }
  
  